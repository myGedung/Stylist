{
	"name": "mygedung/stylist",
	"description": "Laravel 5 theming package.",
	"keywords": [
		"stylist",
		"theme",
		"laravel",
		"mygedung"
	],
	"license": "MIT",
	"require": {
		"php": ">=7.0.0",
        "laravelcollective/html": "~5.5",
        "illuminate/support": "~5.5"
	},
	"require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "~3.5",
        "phpunit/phpunit": "~6.4"
	},
	"autoload": {
		"psr-4": {
            "myGedung\\Stylist\\": "src/Stylist",
            "Tests\\": "tests"
		}
	},
	"extra": {
		"component": "package",
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	},
    "minimum-stability": "dev",
    "prefer-stable": true
}
